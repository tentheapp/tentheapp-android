<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- pressed -->
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:width="3dip" android:color="@color/com_adobe_image_white_semi_transparent" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>

    <!-- checked -->
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/com_adobe_image_white_semi_transparent" />
        </shape>
    </item>

    <!-- selected -->
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/com_adobe_image_white_semi_transparent" />
        </shape>
    </item>

    <!-- diabled -->
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <stroke android:width="3dip" android:color="@color/com_adobe_image_white_semi_transparent_disabled" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>
