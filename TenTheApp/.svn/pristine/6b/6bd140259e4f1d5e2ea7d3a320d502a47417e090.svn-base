<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- default state -->

    <item android:drawable="@drawable/com_adobe_image_flat_button_blue_transparent_shape_enabled" android:state_enabled="true"
        android:state_pressed="false" />

    <!-- pressed state -->
    <item android:drawable="@drawable/com_adobe_image_flat_button_blue_transparent_shape_pressed" android:state_enabled="true"
        android:state_pressed="true" />

    <!--disabled state -->
    <item android:drawable="@drawable/com_adobe_image_flat_button_blue_transparent_shape_disabled" android:state_enabled="false" />
</selector>