<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical">

	<TextView
		android:id="@+id/adobe_csdk_proxy_warningMsg"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:layout_marginLeft="20dp"
		android:layout_marginRight="20dp"
		android:ems="10"
		android:textColor="@color/primary_dark_material_light"
		android:textColorHint="@color/adobe_csdk_asset_browser_light_text">
	</TextView>

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:layout_marginTop="20dp"
		android:layout_marginLeft="20dp"
		android:layout_marginRight="20dp"
		android:ems="15"
		android:text="@string/adobe_csdk_proxy_username_label"
		android:textColor="@color/primary_dark_material_dark"
		android:textColorHint="@color/adobe_csdk_asset_browser_light_text">
	</TextView>


	<EditText
		android:id="@+id/adobe_csdk_proxy_userName"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:layout_marginLeft="20dp"
		android:layout_marginRight="20dp"
		android:ems="10"
		android:inputType="text"
		android:singleLine="true"
		android:textColor="@color/primary_dark_material_dark"
		android:textColorHint="@color/adobe_csdk_asset_browser_light_text"></EditText>

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:layout_marginTop="20dp"
		android:layout_marginLeft="20dp"
		android:layout_marginRight="20dp"
		android:ems="15"
		android:text="@string/adobe_csdk_proxy_passwd_label"
		android:textColor="@color/primary_dark_material_dark"
		android:textColorHint="@color/adobe_csdk_asset_browser_light_text">
	</TextView>

	<EditText
		android:id="@+id/adobe_csdk_proxy_passWord"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:layout_marginLeft="20dp"
		android:layout_marginRight="20dp"
		android:ems="10"
		android:inputType="text"
		android:singleLine="true"
		android:textColor="@color/primary_dark_material_dark"
		android:password="true"
		android:textColorHint="@color/adobe_csdk_asset_browser_light_text"></EditText>


</LinearLayout>