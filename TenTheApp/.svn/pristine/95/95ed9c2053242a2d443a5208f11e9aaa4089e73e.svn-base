<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- disabled -->
    <item android:state_enabled="false" android:color="@color/com_adobe_image_color_blue_disabled" />

    <!-- selected -->
    <item android:state_selected="true" android:color="@color/com_adobe_image_color_blue_full_pressed" />

    <!-- pressed -->
    <item android:state_pressed="true" android:color="@color/com_adobe_image_color_blue_full_pressed" />

    <!-- default -->
    <item android:state_window_focused="false" android:color="@color/com_adobe_image_color_blue_full" />
    <item android:color="@color/com_adobe_image_color_blue_full" />

</selector>