<?xml version="1.0" encoding="utf-8"?>

<!-- Editor bottombar tool item layout -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/com_adobe_image_bottombar_tool_item_min_width"
    android:layout_height="match_parent"
    android:background="@drawable/com_adobe_image_bottombar_tool_item_background"
    android:gravity="center"
    android:minWidth="@dimen/com_adobe_image_bottombar_tool_item_min_width"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/AdobeImageHighlightImageButton11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/com_adobe_image_tool_ic_blemish" />

    <com.adobe.android.ui.view.AdobeTextView
        android:id="@+id/AdobeTextView11"
        style="@style/AdobeImageWidget.CircleButtonText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/com_adobe_image_bottombar_tool_item_text_margin"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true" />

</LinearLayout>
