<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/com_adobe_image_bottombar_default_panel_padding"
    android:gravity="center"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.adobe.creativesdk.aviary.widget.AdobeImageHighlightImageButton
                android:id="@+id/AdobeImageHighlightImageButton12"
                style="@style/AdobeImageWidget.CircleImageButton.NonToggle"
                app:adobe_checked="false"
                app:adobe_toggleable="false"
                android:src="@drawable/com_adobe_image_ic_rotate_left"
                android:contentDescription="@string/feather_acc_rotate_ccw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/com_adobe_image_circle_button_margin_right" />

            <com.adobe.creativesdk.aviary.widget.AdobeImageHighlightImageButton
                android:id="@+id/AdobeImageHighlightImageButton13"
                style="@style/AdobeImageWidget.CircleImageButton.NonToggle"
                app:adobe_checked="false"
                app:adobe_toggleable="false"
                android:src="@drawable/com_adobe_image_ic_rotate_right"
                android:contentDescription="@string/feather_acc_rotate_cw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>


        <com.adobe.android.ui.view.AdobeTextView
            style="@style/AdobeImageWidget.CircleButtonText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/feather_rotate" />
    </LinearLayout>


    <LinearLayout
        android:layout_marginLeft="20dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.adobe.creativesdk.aviary.widget.AdobeImageHighlightImageButton
                android:id="@+id/AdobeImageHighlightImageButton14"
                style="@style/AdobeImageWidget.CircleImageButton.NonToggle"
                app:adobe_checked="false"
                app:adobe_toggleable="false"
                android:src="@drawable/com_adobe_image_ic_flip_horizontal"
                android:contentDescription="@string/feather_acc_flip_h"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/com_adobe_image_circle_button_margin_right" />

            <com.adobe.creativesdk.aviary.widget.AdobeImageHighlightImageButton
                android:id="@+id/AdobeImageHighlightImageButton15"
                style="@style/AdobeImageWidget.CircleImageButton.NonToggle"
                app:adobe_checked="false"
                app:adobe_toggleable="false"
                android:src="@drawable/com_adobe_image_ic_flip_vertical"
                android:contentDescription="@string/feather_acc_flip_v"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>

        <com.adobe.android.ui.view.AdobeTextView
            style="@style/AdobeImageWidget.CircleButtonText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/feather_flip" />
    </LinearLayout>


</LinearLayout>